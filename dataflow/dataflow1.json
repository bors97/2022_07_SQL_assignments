{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RawReviewsCSV",
						"type": "DatasetReference"
					},
					"name": "Reviews"
				},
				{
					"dataset": {
						"referenceName": "RawUsersCSV",
						"type": "DatasetReference"
					},
					"name": "Users"
				},
				{
					"dataset": {
						"referenceName": "RawPlatformsCSV",
						"type": "DatasetReference"
					},
					"name": "Platforms"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "TransformationReviewsCSV",
						"type": "DatasetReference"
					},
					"name": "ReviewsOutput"
				},
				{
					"dataset": {
						"referenceName": "TransformationUsersCSV",
						"type": "DatasetReference"
					},
					"name": "UsersOutput"
				},
				{
					"dataset": {
						"referenceName": "TransformationPlatformsCSV",
						"type": "DatasetReference"
					},
					"name": "PlatformsOutput"
				}
			],
			"transformations": [
				{
					"name": "ReviewsTransform"
				},
				{
					"name": "UsersTransform"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          user_id as short,",
				"          course_id as short,",
				"          feedback as string,",
				"          liked as boolean,",
				"          ranking_score as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false,",
				"     multiLineRow: true) ~> Reviews",
				"source(output(",
				"          user_id as short,",
				"          employer_number as short,",
				"          creation_date as string,",
				"          username as string,",
				"          password as string,",
				"          level as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Users",
				"source(output(",
				"          platform_id as short,",
				"          courses as short,",
				"          platform_name as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Platforms",
				"Reviews derive(feedback = iifNull(feedback, \"No feedback was given.\", feedback)) ~> ReviewsTransform",
				"Users derive(creation_date = toDate(creation_date),",
				"          level = iifNull(level, \"Not yet assigned\", level)) ~> UsersTransform",
				"Platforms derive(platform_name = iifNull(platform_name, \"Untitled\", platform_name),",
				"          url = ERROR_FUNCTION('')) ~> derivedColumn1",
				"ReviewsTransform sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ReviewsOutput",
				"UsersTransform sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> UsersOutput",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> PlatformsOutput"
			]
		}
	}
}